package model;

public class Lastbutton {
	private int value;
	public Lastbutton(int value) {
		this.value=value;
	}
	public int getValue() {
		return value;
	}
	public void setValue(int value) {
		this.value = value;
	}
	public void setvalue_1() 
	{
		this.setValue(1);
	}
	public void setvalue_2() 
	{
		this.setValue(2);
	}
	public void setvalue_3() 
	{
		this.setValue(3);
	}
	public void setvalue_4() 
	{
		this.setValue(4);
	}
}
