package test;

import model.count;
import view.countview;

public class test {
	public static void main(String[] args) {
		count ct= new count();
		ct.increment();
		ct.increment();
		ct.increment();
		System.out.println(ct.getValue());
		ct.decrement();
		System.out.println(ct.getValue());
		new countview();
	}
}
